{
  "name": "JazzApp",
  "version": "0.0.1",
  "description": "Energymost Project",
  "scripts": {
    "test": "jest",
    "dev": "webpack-dev-server --config webpack-hot-dev-server.config.js --hot --progress --colors --port 3000 --inline",
    "build": "webpack --config webpack-production.config.js --progress --profile --colors > ret.log",
    "start": "supervisor -w ./src/server/ ./src/server/server.js"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": [
      "<rootDir>/src/app"
    ],
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "jsx",
      "json",
      "js"
    ],
    "unmockedModulePathPatterns": [
      "react","material-ui","superagent"
    ]
  },
  "devDependencies": {
    "babel-core": "^4.7.1",
    "babel-jest": "^4.0.0",
    "babel-loader": "^4.1.0",
    "css-loader": "^0.9.0",
    "events": "^1.0.2",
    "extract-text-webpack-plugin": "^0.3.5",
    "file-loader": "^0.8.1",
    "hapi": "^8.2.0",
    "html-loader": "^0.2.2",
    "jest-cli": "^0.4.0",
    "keymirror": "^0.1.1",
    "less": "^2.1.2",
    "less-loader": "^2.0.0",
    "null-loader": "^0.1.0",
    "object-assign": "^2.0.0",
    "react-hot-loader": "^1.1.7",
    "react-proxy-loader": "^0.3.1",
    "style-loader": "^0.8.2",
    "supervisor": "^0.6.0",
    "url-loader": "^0.5.5",
    "webpack": "1.7.2",
    "webpack-dev-server": "1.7.0"
  },
  "dependencies": {
    "material-ui": "0.7.5",
    "superagent": "^1.0.0",
    "react": "0.13.2",
    "immutable":"^3.7.2",
    "flux": "2.0.3",
    "react-router": "0.13.3",
    "react-tap-event-plugin": "0.1.6",
    "classnames": "2.1.0",
    "react-grid-layout": "^0.8.3",
    "moment":"^2.10.0"
  },
  "jshintConfig":{
      "esnext":true,
      "moz":true,
      "browser":true,
      "node":true,
      "jasmine":true,
      "predef": [ "I18N","jest" ]
  }

}
