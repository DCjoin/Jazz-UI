{
  "name": "",
  "version": "0.0.1",
  "description": "Energymost Project",
  "scripts": {
    "lint": "eslint --ext jsx src/app",
    "test": "karma start",
    "dev": "webpack-dev-server --config webpack-hot-dev-server.config.js --hot --progress --colors --port 3000 --inline",
    "build": "webpack --config webpack-production.config.js --progress --profile --colors > ret.log",
    "start": "supervisor -w ./src/server/ ./src/server/server.js"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": [
      "<rootDir>/src/app"
    ],
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "jsx",
      "json",
      "js"
    ],
    "unmockedModulePathPatterns": [
      "react","material-ui","superagent"
    ]
  },
  "devDependencies": {
    "babel-polyfill":"6.0.2",
    "babel-core": "5.4.7",
    "babel-loader": "5.1.3",
    "chai": "3.4.1",
    "css-loader": "0.14.4",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.16.1",
    "extract-text-webpack-plugin": "0.8.1",
    "file-loader": "0.8.4",
    "hapi": "8.2.0",
    "html-loader": "0.3.0",
    "karma": "0.13.19",
    "karma-chrome-launcher": "0.2.2",
    "karma-cli": "0.1.2",
    "karma-coverage": "0.5.3",
    "karma-mocha": "0.2.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "less": "2.5.1",
    "less-loader": "2.2.0",
      "mocha": "2.3.4",
    "null-loader": "0.1.1",
    "react-hot-loader": "1.2.7",
    "react-proxy-loader": "0.3.1",
    "style-loader": "0.12.3",
    "supervisor": "0.6.0",
    "url-loader": "0.5.6",
    "webpack": "1.9.10",
    "webpack-dev-server": "1.9.0"
  },
  "dependencies": {
    "numeral": "1.5.3",
    "material-ui": "0.12.3",
    "object-assign": "2.0.0",
    "superagent": "1.0.0",
    "events": "1.0.2",
    "keymirror": "0.1.1",
    "react": "0.13.3",
    "immutable":"3.7.2",
    "flux": "2.0.3",
    "react-router": "0.13.3",
    "react-tap-event-plugin": "0.1.7",
    "classnames": "2.1.1",
    "lodash":"3.10.1",
    "react-grid-layout": "0.8.3",
    "moment":"2.10.6",
    "react-dragula":"1.0.12",
    "react-token-autocomplete":"0.4.0"
  },
  "jshintConfig":{
      "esnext":true,
      "moz":true,
      "browser":true,
      "node":true,
      "jasmine":true,
      "predef": [ "I18N","jest" ]
  }

}
