{
  "name": "",
  "version": "0.0.1",
  "description": "Energymost Project",
  "scripts": {
    "lint": "eslint --ext jsx src/app",
    "test:coverage": "karma start",
    "test:s": "webpack --config ./conf/webpack-test-single.config.js",
    "test:t": "mocha ./coverage/test.bundle.js --watch",
    "test:m": "mocha-webpack --webpack-config webpac-single-ut.config.js --watch \"test/__tests__/stores/KPI/KPIStore-test.jsx\"",
    "dev": "webpack-dev-server --config webpack-hot-dev-server.config.js --hot --progress --colors --port 3000 --inline",
    "dev-all": "concurrently --kill-others \"npm run dev\" \"npm run start\"",
    "build": "webpack --config webpack-production.config.js --progress --profile --colors > ret.log",
    "start": "supervisor -w ./src/server/ ./src/server/server.js",
    "build-test": "webpack --config webpack-test.config.js --progress --profile --colors > ret-test.log"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": [
      "<rootDir>/src/app"
    ],
    "testFileExtensions": [
      "js"
    ],
    "moduleFileExtensions": [
      "jsx",
      "json",
      "js"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "material-ui",
      "superagent"
    ]
  },
  "devDependencies": {
    "babel-core": "6.4.5",
    "babel-eslint": "^7.1.1",
    "babel-loader": "6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "6.0.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "3.4.1",
    "concurrently": "^2.0.0",
    "css-loader": "0.14.4",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.16.1",
    "extract-text-webpack-plugin": "0.8.1",
    "file-loader": "0.8.4",
    "hapi": "15.2.0",
    "html-loader": "0.3.0",
    "isparta-loader": "^2.0.0",
    "karma": "0.13.19",
    "karma-chrome-launcher": "0.2.2",
    "karma-cli": "0.1.2",
    "karma-coverage": "0.5.3",
    "karma-mocha": "0.2.1",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.7.0",
    "less": "2.5.1",
    "less-loader": "2.2.0",
    "mocha": "2.3.4",
    "node-libs-browser": "0.6.0",
    "node-noop": "1.0.0",
    "null-loader": "0.1.1",
    "react-hot-loader": "1.2.7",
    "react-proxy-loader": "0.3.1",
    "react-tap-event-plugin": "^1.0.0",
    "style-loader": "0.12.3",
    "supervisor": "0.6.0",
    "url-loader": "0.5.6",
    "wallaby-webpack": "0.0.26",
    "webpack": "1.9.10",
    "webpack-dev-server": "1.9.0"
  },
  "dependencies": {
    "numeral": "1.5.3",
    "material-ui": "0.16.1",
    "object-assign": "2.0.0",
    "superagent": "1.0.0",
    "events": "1.0.2",
    "keymirror": "0.1.1",
    "react": "15.3.2",
    "react-dom": "15.3.2",
    "react-addons-css-transition-group": "15.3.2",
    "immutable": "3.7.2",
    "flux": "2.0.3",
    "react-router": "3.0.0",
    "classnames": "2.1.1",
    "lodash": "3.10.1",
    "react-grid-layout": "0.13.2",
    "moment": "2.10.6",
    "react-dragula": "1.0.12"
  },
  "jshintConfig": {
    "esnext": true,
    "moz": true,
    "browser": true,
    "node": true,
    "jasmine": true,
    "predef": [
      "I18N",
      "jest"
    ]
  }
}
